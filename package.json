{
  "name": "express-best",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:docker": "nodemon --legacy-watch --watch src --ext ts,ejs,json --exec tsx src/server.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.ejs dist",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "lint": "eslint .",
    "fmt": "prettier -w .",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:clear": "tsx src/scripts/seed.ts clear",
    "db:reset": "tsx src/scripts/seed.ts reset",
    "db:status": "tsx src/scripts/db-status.ts status",
    "db:collections": "tsx src/scripts/db-status.ts collections",
    "db:migrate": "tsx src/scripts/migrate.ts up",
    "db:rollback": "tsx src/scripts/migrate.ts down",
    "db:migrations": "tsx src/scripts/migrate.ts list"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "puppeteer": "^24.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zod-openapi": "^5.4.3"
  }
}
